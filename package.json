{
  "name": "@ludic/ein",
  "version": "0.0.1",
  "description": "Entity Component System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ludic/ein.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "directories": {
    "example": "example"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "parcel watch --no-autoinstall ./src/index.ts",
    "clean": "rimraf ./dist",
    "build": "parcel build --no-autoinstall ./src/index.ts --no-cache",
    "bnt": "npm run build && npm run test",
    "example": "parcel ./examples/index.html --no-autoinstall --no-cache -p 8080",
    "test": "ts-mocha tests/**/*.spec.ts",
    "test:e2e": "ts-mocha tests/e2e/**/*.spec.ts",
    "test:performance": "ts-mocha tests/performance/**/*.spec.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "parcel": "^2.0.0-alpha.3.2",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {}
}
